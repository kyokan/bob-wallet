{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "electron": "6.0.7"
        },
        "debug": true
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ],
    "@babel/plugin-syntax-bigint"
  ]
}
