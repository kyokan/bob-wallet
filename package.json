    "electron": "4.0.1",
    "electron-builder": "20.34.0",
    "electron-devtools-installer": "2.2.4",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.7.0",
    "enzyme-to-json": "3.3.4",
    "eslint": "5.9.0",
